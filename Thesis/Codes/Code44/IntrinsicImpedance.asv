clc;
clear all;
f=1e9;
mu0=4*pi*(1e-7);% (H/m)
eps0=8.854187817e-12;% (F/m)
indmu=1;
indsig=1;
for mur=1:(1e3):(1e4)
    for resistivity=(1e5):(1e9):(1e11)
        sigma=1/resistivity;
        Z(indmu,indsig)=sqrt((i*2*pi*f*mur*mu0)/(sigma+i*2*pi*f*eps0));
        
        indsig=indsig+1;
    end
    indmu=indmu+1;
end
hold on;
surf(1:indmu,1:indsig,abs(Z))
grid on;
xlabel('Relative Permeability \mur')
ylabel('Electrical Conductivity \sigma')
zlabel('Intrinsic Impedance \Omega')